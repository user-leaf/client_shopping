<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context="com.bjaiyouyou.thismall.activity.BindingModeOfPaymentActivity"
    android:orientation="vertical">
    <com.bjaiyouyou.thismall.widget.IUUTitleBar
        android:id="@+id/title_bank_card"
        android:layout_width="match_parent"
        android:layout_height="@dimen/height_top_bar"
        app:iuuTitleBarLeftImage="@mipmap/nav_back_white"
        app:iuuTitleBarTitle="银行卡">
    </com.bjaiyouyou.thismall.widget.IUUTitleBar>
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <include layout="@layout/layout_not_login"/>
        <ListView
            android:id="@+id/lv_band_card"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
        </ListView>
    </FrameLayout>

</LinearLayout>
