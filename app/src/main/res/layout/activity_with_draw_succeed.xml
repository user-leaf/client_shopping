<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context="com.bjaiyouyou.thismall.activity.WithDrawSucceedActivity">
    <com.bjaiyouyou.thismall.widget.IUUTitleBar
        android:id="@+id/title_withdraw_succeed"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:iuuTitleBarTitle="换取成功"
        app:iuuTitleBarLeftImage="@mipmap/nav_back_white"
        android:layout_gravity="start"
        />
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_gravity="center"
        >
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/list_present_success"/>
        <TextView
            android:layout_marginTop="30dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="恭喜您，换取成功！"
            android:textColor="@color/app_red"
            android:textStyle="bold"
            />
    </LinearLayout>




</FrameLayout>
