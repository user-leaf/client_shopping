<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context="shop.imake.activity.IntegralDetailActivity"
    android:orientation="vertical"
    android:background="#eee"
    >
    <shop.imake.widget.IUUTitleBar
        android:id="@+id/title_integral_detail"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:iuuTitleBarTitle="积分详情"
        app:iuuTitleBarLeftImage="@mipmap/nav_back_white"
        />
    <com.handmark.pulltorefresh.library.PullToRefreshListView
        android:layout_marginTop="7px"
        android:id="@+id/lv_integer_detail"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        />
</LinearLayout>
