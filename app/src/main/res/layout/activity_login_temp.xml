<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".activity.LoginTempActivity">

    <EditText
        android:id="@+id/login_new_et_phone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="手机号"
        android:text="18100008888"
        />

    <EditText
        android:id="@+id/login_new_et_password"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="密码"
        android:text="secret"
        />

    <Button
        android:id="@+id/login_new_btn_login"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="登录"
        />

    <Button
        android:id="@+id/login_new_btn_print_token"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="打印token"
        />
</LinearLayout>
